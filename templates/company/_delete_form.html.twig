<form method="post" hidden id="form_delete_company" action="{{ path('company_delete', {'code': company.code}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ company.code) }}">
    <button class="btn">Delete</button>
</form>
