{{ form_start(form) }}
    {{ form_row(form.code) }}
    {{ form_row(form.gender) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.birthDate) }}
    {{ form_row(form.profession) }}
    {{ form_row(form.Company) }}
    {{ form_row(form.workName) }}
    {{ form_row(form.decisionMaking) }}
    {{ form_row(form.salesperson) }}
    {{ form_row(form.mobilePhone) }}
    {{ form_row(form.phone) }}
    {{ form_row(form.email) }}
    {{ form_row(form.linkedin) }}
    {{ form_row(form.facebook) }}
    {{ form_row(form.twitter) }}
    {% if vich_uploader_asset(contact, 'imageFile') is null %}
        <img alt src="">
    {% else %}
        <img src="{{ vich_uploader_asset(contact, 'imageFile') | imagine_filter('miniature100x100') }}" alt>
    {% endif %}
    {{ form_row(form.imageFile) }}
    {{ form_row(form.comment) }}
    {{ form_row(form.optInNewsletter) }}
    {{ form_row(form.optInOffresCommercial) }}

    {{ form_row(form.status) }}
    <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
